@model IEnumerable<Entities.Entities.LogSistema>

@{
    ViewData["Title"] = "Lista";
}

<h1>Lista</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JsonInformacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoLog)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeController)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeAction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JsonInformacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoLog)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeController)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeAction)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.UserName)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.IdLogSistema" class="btn btn-info">Detalhes</a>
            </td>
        </tr>
}
    </tbody>
</table>
