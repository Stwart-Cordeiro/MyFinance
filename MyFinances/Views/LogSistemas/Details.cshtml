@model Entities.Entities.LogSistema

@{
    ViewData["Title"] = "Detalhes";
}

<div>
    <h4> Detalhes - Log Sistema</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.JsonInformacao)
        </dt>
        <dd class="col-sm-10">
            <div id="json"></div>

            <input asp-for="JsonInformacao" type="hidden" />
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TipoLog)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoLog)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomeController)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeController)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomeAction)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeAction)
        </dd>
        <dt class="col-sm-2">
            Usuario Id
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Usuario.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataErro)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataErro)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Usuario.UserName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-primary">Voltar</a>
</div>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/JsonView.js"></script>
<script src="~/js/logSistema.js"></script>