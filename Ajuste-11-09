    CONSTRAINT `PK_PlanoConta` PRIMARY KEY (`IdPlanoConta`),
    CONSTRAINT `FK_PlanoConta_AspNetUsers_UserId` FOREIGN KEY (`UserId`) REFERENCES `AspNetUsers` (`Id`) ON DELETE RESTRICT
